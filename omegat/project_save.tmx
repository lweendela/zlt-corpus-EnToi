  <body>
<!-- Default translations -->
    <tu>
      <tuv lang="en">
        <seg>My father got fired.</seg>
      </tuv>
      <tuv lang="toi" changeid="lweendela" changedate="20250804T193236Z" creationid="lweendela" creationdate="20250804T193236Z">
        <seg>Bataata baka tandwa ncito.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv lang="en">
        <seg>My father has many houses.</seg>
      </tuv>
      <tuv lang="toi" changeid="lweendela" changedate="20250804T193352Z" creationid="lweendela" creationdate="20250804T193352Z">
        <seg>Bataata ba laa maanda manji.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv lang="en">
        <seg>My father is not sick.</seg>
      </tuv>
      <tuv lang="toi" changeid="lweendela" changedate="20250804T193823Z" creationid="lweendela" creationdate="20250804T193823Z">
        <seg>Bataata ta ba cisidwe.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv lang="en">
        <seg>My girlfriend is a teacher.</seg>
      </tuv>
      <tuv lang="toi" changeid="lweendela" changedate="20250804T193925Z" creationid="lweendela" creationdate="20250804T193925Z">
        <seg>Musimbi wangu mwiiyi.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv lang="en">
        <seg>My girlfriend is crying.</seg>
      </tuv>
      <tuv lang="toi" changeid="lweendela" changedate="20250804T194118Z" creationid="lweendela" creationdate="20250804T194118Z">
        <seg>Musimbi wangu u la lila.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv lang="en">
        <seg>My girlfriend was crying.</seg>
      </tuv>
      <tuv lang="toi" changeid="lweendela" changedate="20250804T194336Z" creationid="lweendela" creationdate="20250804T194336Z">
        <seg>Musimbi wangu waka li kulila.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv lang="en">
        <seg>My granddaughter is listening to my wife.</seg>
      </tuv>
      <tuv lang="toi" changeid="lweendela" changedate="20250804T195820Z" creationid="lweendela" creationdate="20250804T195820Z">
        <seg>Muzyukulu wangu u swiilila ku mukaintu wangu.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv lang="en">
        <seg>My hands and legs are swollen.</seg>
      </tuv>
      <tuv lang="toi" changeid="lweendela" changedate="20250804T200501Z" creationid="lweendela" creationdate="20250804T200501Z">
        <seg>Maanza a maulu aangu a li zimbide.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv lang="en">
        <seg>My hands are bound.</seg>
      </tuv>
      <tuv lang="toi" changeid="lweendela" changedate="20250804T200525Z" creationid="lweendela" creationdate="20250804T200525Z">
        <seg>Maanza aangu a li angidwe.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv lang="en">
        <seg>My hands are clean.</seg>
      </tuv>
      <tuv lang="toi" changeid="lweendela" changedate="20250804T200603Z" creationid="lweendela" creationdate="20250804T200603Z">
        <seg>Maanza aangu a la salala.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv lang="en">
        <seg>My hands are cold.</seg>
      </tuv>
      <tuv lang="toi" changeid="lweendela" changedate="20250804T200642Z" creationid="lweendela" creationdate="20250804T200642Z">
        <seg>Maanza aangu a la tontola.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv lang="en">
        <seg>My hands are dirty.</seg>
      </tuv>
      <tuv lang="toi" changeid="lweendela" changedate="20250804T200707Z" creationid="lweendela" creationdate="20250804T200707Z">
        <seg>Maanza aangu a laa tombe.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv lang="en">
        <seg>No one knows what happened.</seg>
      </tuv>
      <tuv lang="toi" changeid="lweendela" changedate="20250803T120945Z" creationid="lweendela" creationdate="20250803T120945Z">
        <seg>Kunyina uu zyi caka citika.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv lang="en">
        <seg>No one knows your secrets.</seg>
      </tuv>
      <tuv lang="toi" changeid="lweendela" changedate="20250804T185856Z" creationid="lweendela" creationdate="20250804T185856Z">
        <seg>Kunyina uu zyi maseseke aako.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv lang="en">
        <seg>No one knows.</seg>
      </tuv>
      <tuv lang="toi" changeid="lweendela" changedate="20250803T121210Z" creationid="lweendela" creationdate="20250803T121210Z">
        <seg>Kunyina uu zyi.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv lang="en">
        <seg>No one laughed.</seg>
      </tuv>
      <tuv lang="toi" changeid="lweendela" changedate="20250803T121131Z" creationid="lweendela" creationdate="20250803T121131Z">
        <seg>Kunyina ooka seka.</seg>
      </tuv>
    </tu>
